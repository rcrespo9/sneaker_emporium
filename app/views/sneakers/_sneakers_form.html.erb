<%= form_for sneaker do |f| %>
        <div>
                <%= f.label :name %>
                <%= f.text_field :name %>
        </div>
        <div>
                <%= f.label :price %>
                <%= f.text_field :price %>
        </div>
        <div>
                <%= f.collection_select(:brand_id, Brand.all, :id, :name) %>
        </div>
        <div>
                <%= f.submit %>
        </div>
<% end %>