<%= form_for store do |f| %>
        <div>
                <%= f.label :name %>
                <%= f.text_field :name %>
        </div>
        <div>
                <%= f.collection_select :sneaker_ids, Sneaker.all, :id, :name, {name: 'line_item[sneaker_ids][]'} %>
        </div>
        <div>
                <%= f.submit %>
        </div>
<% end %>